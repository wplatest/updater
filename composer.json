{
    "name": "wplatest/updater",
    "description": "A simple and lightweight package to update your WordPress plugins and themes from a private repository.",
    "homepage": "https://github.com/wplatest/updater/",
    "type": "library",
    "version": "1.2.5",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Chris Jayden",
            "homepage": "https://chrisjayden.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "WpLatest\\Updater\\": "src/"
        }
    },
    "require": {
        "php": "^8.0"
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpcompatibility/php-compatibility": "^9.3",
        "wp-coding-standards/wpcs": "^3.1",
        "php-stubs/wordpress-stubs": "^6.4"
    },
    "scripts": {
        "phpcs": [
            "phpcs --extensions=php -s -p"
        ],
        "phpcbf": [
            "phpcbf -p --extensions=php"
        ]
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}